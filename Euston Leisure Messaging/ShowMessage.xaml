<Window x:Class="Euston_Leisure_Messaging.ShowMessage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Euston_Leisure_Messaging"
        mc:Ignorable="d"
        Title="ShowMessage" Height="350" Width="525">
    <ScrollViewer HorizontalAlignment="Center" Height="289" VerticalAlignment="Top" Width="490" Margin="20,13,7,0" RenderTransformOrigin="0.5,0.5" >
        <ScrollViewer.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform AngleY="0.249"/>
                <RotateTransform/>
                <TranslateTransform Y="0.777"/>
            </TransformGroup>
        </ScrollViewer.RenderTransform>
        <TextBlock x:Name="outputText" TextWrapping="Wrap" RenderTransformOrigin="0.496,0.51" Height="268" Width="444"/>
    </ScrollViewer>
</Window>
