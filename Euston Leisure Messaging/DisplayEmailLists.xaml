<Window x:Class="Euston_Leisure_Messaging.DisplayEmailLists"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Euston_Leisure_Messaging"
        mc:Ignorable="d"
       Title="DisplayEmailLists" Height="350" Width="480">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="160*"/>
            <RowDefinition Height="159*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="167*"/>
            <ColumnDefinition Width="125*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="textBlock_SIR" HorizontalAlignment="Left" Margin="66,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="187" Width="140" Grid.RowSpan="2"/>
        <TextBlock x:Name="textBlock_URL" HorizontalAlignment="Left" Margin="29,82,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="187" Width="140" Grid.RowSpan="2" Grid.Column="1"/>
        <Label x:Name="lbl_SIR" Content="SIR list" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="66,51,64,0" Width="140"/>
        <Label x:Name="lbl_URL" Content="URL list" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="29,51,33,0" Width="140" Grid.Column="1"/>

    </Grid>
</Window>
